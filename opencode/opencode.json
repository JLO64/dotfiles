{
  "$schema": "https://opencode.ai/config.json",
  "disabled_providers": [
    "openai",
    "groq"
  ],
  "provider": {
    "lmstudio": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "LM Studio (local)",
      "options": {
        "baseURL": "http://127.0.0.1:1234/v1"
      },
      "models": {
        "openai/gpt-oss-20b": {
          "name": "GPT OSS 20B"
        }
      }
    },
    "openrouter": {
      "models": {
        "openai/gpt-oss-120b": {
          "reasoning": true,
          "options": {
            "reasoning_effort": "high",
            "provider": {
              "order": [
                "groq"
              ],
              "allow_fallbacks": true
            }
          }
        },
        "openai/gpt-5": {
          "reasoning": true,
          "options": {
            "reasoning_effort": "high"
          }
        },
        "openai/gpt-5-mini": {
          "reasoning": true,
          "options": {
            "reasoning_effort": "high"
          }
        }
      }
    }
  }
}
